{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "prettier",
    "import",
    "@typescript-eslint"
  ],
  "ignorePatterns": [
    "node_modules"
  ],
  "rules": {
    "no-debugger": "off",
    "no-console": 0,
    "class-methods-use-this": "off",
    "@typescript-eslint/no-explicit-any": 2
  }
}